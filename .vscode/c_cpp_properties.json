{
    "configurations": [
        {
            "name": "STM32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Core/Inc",
                "${workspaceFolder}/Drivers/STM32G4xx_HAL_Driver/Inc",
                "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32G4xx/Include",
                "${workspaceFolder}/Drivers/CMSIS/Include",
                "${workspaceFolder}/APP",
                "${workspaceFolder}/APP/tasks/cmd_task",
                "${workspaceFolder}/APP/tasks/motor_task",
                "${workspaceFolder}/APP/tasks/robot_task",
                "${workspaceFolder}/BSP",
                "${workspaceFolder}/BSP/can",
                "${workspaceFolder}/BSP/dwt",
                "${workspaceFolder}/BSP/log",
                "${workspaceFolder}/BSP/usart",
                "${workspaceFolder}/BSP/flash",
                "${workspaceFolder}/BSP/adc",
                "${workspaceFolder}/Middlewares/Third_Party/FreeRTOS/Source/include",
                "${workspaceFolder}/Middlewares/Third_Party/FreeRTOS/Source/CMSIS_RTOS",
                "${workspaceFolder}/Middlewares/Third_Party/FreeRTOS/Source/portable/GCC/ARM_CM4F",
                "${workspaceFolder}/MODULES",
                "${workspaceFolder}/MODULES/motor",
                "${workspaceFolder}/MODULES/motor/core",
                "${workspaceFolder}/MODULES/motor/control",
                "${workspaceFolder}/MODULES/motor/algorithms",
                "${workspaceFolder}/MODULES/motor/calibration",
                "${workspaceFolder}/MODULES/motor/hal",
                "${workspaceFolder}/MODULES/motor/foc_algorithm",
                "${workspaceFolder}/MODULES/motor/foc_algorithm/foc_core",
                "${workspaceFolder}/MODULES/motor/foc_algorithm/transform",
                "${workspaceFolder}/MODULES/motor/foc_algorithm/driver",
                "${workspaceFolder}/MODULES/motor/fsm",
                "${workspaceFolder}/MODULES/motor/safety",
                "${workspaceFolder}/MODULES/motor/state_machine",
                "${workspaceFolder}/MODULES/encoder",
                "${workspaceFolder}/MODULES/encoder/MT6816",
                "${workspaceFolder}/MODULES/led",
                "${workspaceFolder}/MODULES/communication",
                "${workspaceFolder}/MODULES/communication/inovxio",
                "${workspaceFolder}/MODULES/communication/canopen",
                "${workspaceFolder}/MODULES/communication/mit",
                "${workspaceFolder}/MODULES/parameter",
                "${workspaceFolder}/MODULES/vofa",
                "${workspaceFolder}/MODULES/daemon",
                "${workspaceFolder}/MODULES/hal",
                "${workspaceFolder}/MODULES/algorithm",
                "${workspaceFolder}/MODULES/error"
            ],
            "defines": [
                "USE_HAL_DRIVER",
                "STM32G431xx",
                "__weak=__attribute__((weak))",
                "__packed=__attribute__((__packed__))",
                "__GNUC__=11",
                "__GNUC_MINOR__=2",
                "__GNUC_PATCHLEVEL__=1",
                "ARM_MATH_CM4",
                "__FPU_PRESENT=1",
                "__ARM_ARCH_7EM__=1",
                "__ARM_FEATURE_DSP=1"
            ],
            "compilerPath": "arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "compilerArgs": [
                "-mcpu=cortex-m4",
                "-mthumb",
                "-mfpu=fpv4-sp-d16",
                "-mfloat-abi=hard"
            ]
        }
    ],
    "version": 4
}
