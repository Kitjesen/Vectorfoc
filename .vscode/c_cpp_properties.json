{
    "configurations": [
        {
            "name": "STM32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Lib/Core/Inc",
                "${workspaceFolder}/Lib/Drivers/STM32G4xx_HAL_Driver/Inc",
                "${workspaceFolder}/Lib/Drivers/CMSIS/Device/ST/STM32G4xx/Include",
                "${workspaceFolder}/Lib/Drivers/CMSIS/Include",
                "${workspaceFolder}/Lib/Middlewares/Third_Party/FreeRTOS/Source/include",
                "${workspaceFolder}/Lib/Middlewares/Third_Party/FreeRTOS/Source/CMSIS_RTOS",
                "${workspaceFolder}/Lib/Middlewares/Third_Party/FreeRTOS/Source/portable/GCC/ARM_CM4F",
                "${workspaceFolder}/Src/APP",
                "${workspaceFolder}/Src/APP/init",
                "${workspaceFolder}/Src/APP/rtos",
                "${workspaceFolder}/Src/APP/isr",
                "${workspaceFolder}/Src/HAL/interface",
                "${workspaceFolder}/Src/HAL/stm32g4",
                "${workspaceFolder}/Src/HAL/bsp",
                "${workspaceFolder}/Src/HAL/encoder",
                "${workspaceFolder}/Src/ALGO",
                "${workspaceFolder}/Src/ALGO/motor",
                "${workspaceFolder}/Src/ALGO/control",
                "${workspaceFolder}/Src/ALGO/foc",
                "${workspaceFolder}/Src/ALGO/pid",
                "${workspaceFolder}/Src/ALGO/observer",
                "${workspaceFolder}/Src/ALGO/trajectory",
                "${workspaceFolder}/Src/COMM",
                "${workspaceFolder}/Src/COMM/manager",
                "${workspaceFolder}/Src/COMM/executor",
                "${workspaceFolder}/Src/COMM/protocol",
                "${workspaceFolder}/Src/COMM/protocol/inovxio",
                "${workspaceFolder}/Src/COMM/protocol/canopen",
                "${workspaceFolder}/Src/COMM/protocol/mit",
                "${workspaceFolder}/Src/COMM/transport",
                "${workspaceFolder}/Src/UI",
                "${workspaceFolder}/Src/UI/parameter",
                "${workspaceFolder}/Src/UI/vofa",
                "${workspaceFolder}/Src/UI/led",
                "${workspaceFolder}/Src/UI/error"
            ],
            "defines": [
                "USE_HAL_DRIVER",
                "STM32G431xx",
                "__weak=__attribute__((weak))",
                "__packed=__attribute__((__packed__))",
                "__GNUC__=11",
                "__GNUC_MINOR__=2",
                "__GNUC_PATCHLEVEL__=1",
                "ARM_MATH_CM4",
                "__FPU_PRESENT=1",
                "__ARM_ARCH_7EM__=1",
                "__ARM_FEATURE_DSP=1"
            ],
            "compilerPath": "arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "compilerArgs": [
                "-mcpu=cortex-m4",
                "-mthumb",
                "-mfpu=fpv4-sp-d16",
                "-mfloat-abi=hard"
            ]
        }
    ],
    "version": 4
}
